{
  "name": "orphic",
  "version": "1.0.0",
  "description": "Ninth weekly exercise.",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/index.js",
    "build": "rm -rf dist && babel src --out-dir dist --copy-files",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "format": "prettier --write \"src/**/*.js\"",
    "linting": "eslint --ignore-path .eslintignore './src/**/*.{ts,js}'",
    "lint": "npm run linting -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiotrope/orphic.git"
  },
  "author": "aiotrope",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/aiotrope/orphic/issues"
  },
  "homepage": "https://github.com/aiotrope/orphic#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.11.2",
    "morgan": "^1.10.0",
    "winston": "^3.9.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": "12.22.12",
    "npm": "6.14.16"
  }
